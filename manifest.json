{
  "eula": "",
  "vendor": "SAP",
  "license": "",
  "id": "com.sap.sac.kpiwidget_1.x",
  "version": "2.0.0",
  "name": "Widget KPI",
  "newInstancePrefix": "WidgetKPI",
  "description": "Widget de KPI",
  "webcomponents": [
    {
      "kind": "main",
      "tag": "ejemplo-kpi",
      "url": "/main.js",
      "integrity": "sha256-<generated_hash>",
      "ignoreIntegrity": true
    },
    {
      "kind": "styling",
      "tag": "com-sap-sac-exercise-username-styling",
      "url": "/styling.js",
      "integrity": "sha256-<generated_hash>",
      "ignoreIntegrity": true
    }
  ],
  "properties": {
    "title": {
      "type": "string",
      "default": "Título del KPI",
      "description": "Título del KPI"
    },
    "subtitle": {
      "type": "string",
      "default": "Fecha | Actual vs. Pasado | En Años",
      "description": "Texto del subtítulo"
    },
    "information": {
      "type": "string",
      "default": "Actual (<span>Actual - plan</span>)",
      "description": "Texto de la información"
    },
    "real": {
      "type": "string",
      "default": "0",
      "description": "Real KPI"
    },
    "color": {
      "type": "string",
      "default": "#000000",
      "description": "Color of the title text"
    }
  },
  "methods": {
    "setTitle": {
      "description": "Establecer el título del KPI",
      "parameters": [
        {
          "name": "value",
          "type": "string",
          "description": "Valor del título"
        }
      ]
    },
    "setSubtitle": {
      "description": "Establecer el subtítulo del KPI",
      "parameters": [
        {
          "name": "value",
          "type": "string",
          "description": "Valor del subtítulo"
        }
      ]
    },
    "setInformation": {
      "description": "Establecer la información del KPI",
      "parameters": [
        {
          "name": "value",
          "type": "string",
          "description": "Valor de la información"
        }
      ]
    },
    "setReal": {
      "description": "Establecer el Real del KPI",
      "parameters": [
        {
          "name": "value",
          "type": "string",
          "description": "Valor del real"
        }
      ]
    },
    "setResultadoColor": {
      "parameters": [
        {
          "name": "value",
          "type": "string",
          "description": "color del resultado"
        }
      ]
    }
  },
  "events": {},
  "dataBindings": {
      "dataBinding": {
        "feeds": [
          {
            "id": "measures",
            "description": "Medidas real",
            "type": "mainStructureMember"
          },
          {
            "id": "measures2",
            "description": "Medidas Objetivo Acumulado",
            "type": "mainStructureMember"
          }
        ]
      }
    }
}